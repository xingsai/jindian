.container{

    box-sizing: border-box;
    background-color: #fff;

}
/*遮罩弹出框样式*/
.model-style{
    bottom: 0;
    left: 0;
    position: absolute;
    right: 0;
    top: 0;
    display:block;
    background: rgba(200, 200, 200, 1) none repeat scroll 0 0;
    z-index: 1;
}
.model-style .content-wrap{
    background-color:#fff;
    height:300rpx;
    width:80%;
    margin:0 auto;
    overflow:auto;
    -webkit-overflow-scrolling: touch;
    overflow-scrolling: touch;
    margin-top:45%;
    border-radius: 10rpx;
    padding: 40rpx;
    position: relative;
    box-sizing: border-box;
}
.model-style .btn-style-wrap{
    position: absolute;
    bottom:20rpx;
    right:20rpx;
    display:flex;
    justify-content: space-around;
    align-items:center;
    background-color:#fff;
    width:300rpx;
    margin:0 auto;
    border-bottom-left-radius:5px;
    border-bottom-right-radius:5px;
}
.btn-style{
    height:60rpx;
    line-height: 60rpx;
    font-size:32rpx;
    border: none;
    background-color: #fff;
}
.ok-btn{
    color:rgb(26,173,25);
}
