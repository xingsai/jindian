.null-tip {
    height: 520rpx;
    margin: 120rpx 0;
    background-image: url(data:image/png;base64,);
    background-repeat: no-repeat;
    background-position: center;
    background-size: 140rpx 140rpx;
    line-height: 720rpx;
    text-align: center;
    color: #999;
}

.null-action {
    margin: 0 auto;
    width: 200rpx;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    border-radius: 8rpx;
    border: 1px solid #47a86c;
    color: #47a86c;
}

.null-action:active {
    background-color: #47a86c;
    color: #fff;
}